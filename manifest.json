{
    "manifest_version": 2,
    "name": "Lazy Speller",
    "version": "1.0",
    "description": "Lazy speller auto accepts Google's spelling suggestion, when you misspell a search term or sentence, so you don't have to!",
    "content_scripts": [
        {
          "matches": ["https://www.google.com/*"  ],
          "js": ["content.js"]
        }
      ],
      "icons": {
        "16": "images/icons8-spellcheck-16.png",
        "32": "images/icons8-spellcheck-32.png",
        "48": "images/icons8-spellcheck-48.png",
        "128": "images/icons8-spellcheck-100.png"
      }
  }